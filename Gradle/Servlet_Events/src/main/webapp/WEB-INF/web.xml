<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlet_Events</display-name>
  <listener>
  	<listener-class>com.techlabs.listeners.AppContextListener</listener-class>
  </listener>
  <listener>
  	<listener-class>com.techlabs.listeners.AppContextAttributeListener</listener-class>
  </listener>
  <listener>
  	<listener-class>com.techlabs.listeners.AppSessionListener</listener-class>
  </listener>
  <listener>
  	<listener-class>com.techlabs.listeners.AppRequestListener</listener-class>
  </listener>
  <servlet>
  	<description>
  	</description>
  	<display-name>MyServlet</display-name>
  	<servlet-name>MyServlet</servlet-name>
  	<servlet-class>com.techlabs.controllers.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyServlet</servlet-name>
  	<url-pattern>/myesrvlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>